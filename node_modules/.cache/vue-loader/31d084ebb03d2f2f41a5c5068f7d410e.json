</template>\n        <v-list min-height=\"40px\" min-width=\"200px\">\n          <v-list-item v-for=\"(item, i) in items\" :key=\"i\" link>\n            <v-list-item-title>{{ item.title }}</v-list-item-title>\n          </v-list-item>\n        </v-list>\n      </v-menu>\n    </v-toolbar>\n\n    <v-navigation-drawer v-model=\"drawer\" absolute bottom temporary app>\n      <v-list nav dense>\n        <v-list-item-group v-model=\"group\">\n          <v-list-item @click=\"jumpPage('/')\">\n            <v-list-item-title>游戏</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item @click=\"jumpPage('/')\">\n            <v-list-item-title>好友</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item>\n            <v-list-item-title>帮助</v-list-item-title>\n          </v-list-item>\n        </v-list-item-group>\n      </v-list>\n      <template v-slot:append>\n        <div v-if=\"userInfo.username.length === 0\">\n          <div class=\"pa-2\">\n            <v-btn block to=\"/register\"> 注册 </v-btn>\n          </div>\n          <div class=\"pa-2\">\n            <v-btn block to=\"/login\"> 登录 </v-btn>\n          </div>\n        </div>\n        <div v-else>\n          <div class=\"pa-2\">\n            <v-btn block to=\"/profile\"> 个人信息 </v-btn>\n          </div>\n          <div class=\"pa-2\">\n            <v-btn block @click=\"logOut\"> 登出 </v-btn>\n          </div>\n        </div>\n      </template>\n    </v-navigation-drawer>\n\n    <v-card-text> </v-card-text>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\nexport default {\n  name: \"Bar\",\n  data: () => ({\n    drawer: false,\n    group: null,\n    search: false,\n    isLoggedIn: false,\n    items: [{ title: \"关于\" }, { title: \"捐款\" }],\n  }),\n  computed: {\n    ...mapGetters(\"user\", [\"userInfo\", \"token\"]),\n  },\n  watch: {\n    group() {\n      this.drawer = false;\n    },\n  },\n  methods: {\n    ...mapActions(\"user\", [\"LoginOut\"]),\n    isHome(route) {\n      return route.name === \"home\";\n    },\n    jumpPage(route) {\n      if (this.userInfo.name.length === 0) {\n        route = \"/login\";\n      }\n      this.$router.push({ path: route });\n    },\n    logOut() {\n      console.log(this.userInfo);\n      this.LoginOut(this.userInfo);\n    },\n  },\n};\n</script>\n\n<style lang = \"scss\">\n.drawer_header {\n  text-decoration: none;\n  margin: 0;\n  height: 56px;\n  h1 {\n    text-align: center;\n  }\n}\n.title-without-decoration {\n  a {\n    text-decoration: none;\n    text-align: center;\n  }\n}\n</style>"]}]}