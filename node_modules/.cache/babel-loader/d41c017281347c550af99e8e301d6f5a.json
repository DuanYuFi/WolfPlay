A,CAAA,SAAA,CAAA,CADA;AAEA,IAAA,UAFA,wBAEA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,SAAA,CAAA,QAAA,EAAA,EAAA;AACA,YAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,SAAA,CAAA,CAAA;AACA,QAAA,OAAA,CAAA,QAAA,GAAA,GAAA,CAAA,OAAA,CAAA,QAAA,CAAA;AACA,YAAA,GAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,GAAA,CACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EADA,CAEA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AACA,SALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,SATA;AAUA;AACA;AAlBA;AA/BA,CAAA","sourcesContent":["<template>\n  <div class=\"login\">\n    <vue-particles\n      color=\"#dedede\"\n      :particle-opacity=\"0.8\"\n      :particles-number=\"50\"\n      shape-type=\"circle\"\n      :particle-size=\"12\"\n      lines-color=\"#dedede\"\n      :lines-width=\"1\"\n      :line-linked=\"true\"\n      :line-opacity=\"0.4\"\n      :lines-distance=\"150\"\n      :move-speed=\"2\"\n      :hover-effect=\"true\"\n      hover-mode=\"grab\"\n      :click-effect=\"false\"\n    />\n    <v-container class=\"fill-height warpper\" fluid>\n      <v-row align=\"center\" justify=\"center\">\n        <v-col cols=\"12\" sm=\"8\" md=\"4\">\n          <v-card class=\"elevation-12\">\n            <v-toolbar color=\"primary\" dark flat>\n              <v-toolbar-title>登录</v-toolbar-title>\n            </v-toolbar>\n            <v-card-text>\n              <v-form ref=\"loginForm\" v-model=\"valid\">\n                <v-text-field\n                  v-model=\"loginForm.username\"\n                  :rules=\"rules.usernameRules\"\n                  label=\"用户名\"\n                  name=\"login\"\n                  prepend-icon=\"mdi-account\"\n                  type=\"text\"\n                />\n                <v-text-field\n                  id=\"password\"\n                  label=\"密码\"\n                  name=\"password\"\n                  prepend-icon=\"mdi-lock\"\n                  v-model=\"loginForm.password\"\n                  :rules=\"rules.passwordRules\"\n                  :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n                  :type=\"show ? 'text' : 'password'\"\n                  @click:append=\"show = !show\"\n                />\n              </v-form>\n              <!--<v-alert type=\"success\">I'm a success alert.</v-alert>-->\n            </v-card-text>\n            <v-alert v-if=\"invalid\" dense outlined type=\"error\">\n              {{ errorMsg }}\n            </v-alert>\n            <v-card-actions>\n              <v-spacer />\n              <v-btn @click=\"submitForm\" color=\"primary\"> 登录 </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\nimport md5 from \"@/utils/md5.js\";\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      curYear: 0,\n      show: false,\n      show2: false,\n      isWaiting: false,\n      loginForm: {\n        username: \"\",\n        password: \"\",\n      },\n      invalid: false,\n      errorMsg: \"\",\n      rules: {\n        usernameRules: [\n          (v) => !!v || \"请输入用户名\",\n          (v) => (v && v.length >= 3) || \"用户名长度必须大于2\",\n          (v) => (v && v.length <= 16) || \"用户名长度必须小于等于16\",\n        //  (v) => /[a-zA-Z0-9_\\u4e00-\\u9fa5]+/.test(v) || \"非法用户名\",\n        ],\n        passwordRules: [\n          (v) => !!v || \"请输入密码\",\n          (v) => (v && v.length >= 8) || \"密码长度必须大于等于8\",\n        ],\n      },\n    };\n  },\n  created() {\n    this.curYear = new Date().getFullYear();\n  },\n  methods: {\n    ...mapActions(\"user\", [\"LoginIn\"]),\n    submitForm() {\n      if (this.$refs.loginForm.validate()) {\n        var subForm = JSON.parse(JSON.stringify(this.loginForm));\n        subForm.password = md5(subForm.password);\n        const res = this.LoginIn(subForm);\n        res\n          .then((code) => {\n            console.log(code);\n            // alert(\"登陆成功!\");\n            this.$router.push({ name: \"Home\" });\n          })\n          .catch((error) => {\n            this.invalid = true;\n            this.errorMsg = error;\n          });\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.warpper {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -webkit-transform: translate(-50%, -50%);\n  display: flex;\n}\n</style>\n"],"sourceRoot":"src/views"}]}