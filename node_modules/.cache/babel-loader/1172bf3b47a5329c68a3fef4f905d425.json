                id=\"password\"\n                  label=\"密码\"\n                  name=\"password\"\n                  prepend-icon=\"mdi-lock\"\n                  v-model=\"loginForm.password\"\n                  :rules=\"rules.passwordRules\"\n                  :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n                  :type=\"show ? 'text' : 'password'\"\n                  @click:append=\"show = !show\"\n                />\n              </v-form>\n              <!--<v-alert type=\"success\">I'm a success alert.</v-alert>-->\n            </v-card-text>\n            <v-card-actions>\n              <v-spacer />\n              <v-btn\n                @click=\"submitForm\"\n                color=\"primary\"\n              >\n                登录\n              </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex'\nimport { login } from '@/api/user'\nimport md5 from '@/utils/md5.js'\nexport default {\n  name: 'Register',\n  data () {\n    return {\n      show: false,\n      show2: false,\n      isWaiting: false,\n      loginForm: {\n        username: '',\n        password: '',\n      },\n      valid: true,\n      errorMsg: '',\n      rules: {\n        usernameRules: [\n          v => !!v || '请输入用户名',\n          v => (v && v.length >= 3) || '用户名长度必须大于2',\n          v => (v && v.length <= 16) || '用户名长度必须小于等于16',\n          v => /^[a-zA-Z0-9_-]{3,16}$/.test(v) ||  '非法用户名'\n        ],\n        passwordRules: [\n          v => !!v || '请输入密码',\n          v => (v && v.length >= 8) || '密码长度必须大于等于8'\n        ]\n      }\n    }\n  },\n  methods: {\n    ...mapActions('user', ['LoginIn']),\n    async submitForm () {\n      if (this.$refs.loginForm.validate()) {\n        var subForm = JSON.parse(JSON.stringify(this.loginForm));\n        subForm.password = md5(subForm.password)\n        const res = await login(subForm)\n        if (res.code === 0) {\n          console.log('Success')\n          this.$router.push({ name: 'login' })\n        }\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.warpper {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -webkit-transform: translate(-50%, -50%);\n  display: flex;\n}\n</style>\n"],"sourceRoot":"src/views"}]}